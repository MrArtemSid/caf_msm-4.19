&apc_vreg_corner {
	qcom,cpr-voltage-floor = <1150000 1150000 1250000>;
};

&pm8937_l6 {
	regulator-always-on;
};

&spmi_bus {
	qcom,pm8937@0 {
		qcom,power-on@800 {
			qcom,store-hard-reset-reason;
			qcom,pon_1 {
				qcom,support-reset = <1>;
				qcom,s2-timer = <2000>;
				qcom,s2-type = <7>;
			};
		};
	};
};

&sdc2_cmd_on {
	config {
		drive-strength = <12>;
	};
};

&sdc2_data_on {
	config {
		drive-strength = <12>;
	};
};

&pmi_haptic {
	status = "okay";
	qcom,vmax-mv = <2700>;
	wf_0 { 
		qcom,wf-vmax-mv = <3000>;
	};
	wf_1 { 
		qcom,wf-vmax-mv = <3000>; 
	};
	wf_2 { 
		qcom,wf-vmax-mv = <3000>; 
	};
	wf_3 { 
		qcom,wf-vmax-mv = <3000>; 
	};
	wf_4 { 
		qcom,wf-vmax-mv = <3000>; 
	};
	wf_5 { 
		qcom,wf-vmax-mv = <3000>; 
	};
};
